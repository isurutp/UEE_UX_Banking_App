<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uee_banking_app_android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OpenMessage"/>
        <activity android:name=".ViewMessages" />
        <activity android:name=".MessageSent" />
        <activity android:name=".ComposeMessage" />
        <activity android:name=".ResetInstructions" />
        <activity android:name=".Register" />
        <activity android:name=".LoginDetailsMessage" />
        <activity android:name=".ForgotPassword" />

        <activity android:name=".credit_cards"/>
        <activity android:name=".menu_screen" />
        <activity android:name=".sms_verification"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>